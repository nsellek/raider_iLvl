<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <table class="table">
        <tr>
          <th>Name</th>
          <th>Realm</th>
          <th>Spec</th>
        </tr>
        <% @members.each do |member| %>
        <tr>
          <td><%= member['name'] %></td>
          <td><%= member["realm"] %></td>
          <td><%= member['spec']['name'] %></td>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
