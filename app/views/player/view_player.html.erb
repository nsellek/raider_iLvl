<%= button_to "Back", session.delete(:return_to), class: "btn btn-primary", method: :get %>
<div class="center">
  <h3>Items of <%= @player.name %></h3>
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <table class="table">
          <tr>
            <th></th>
            <th class="center">iLvl</th>
            <th class="center">Name</th>
          </tr>
          <% @items.each do |item| %>
          <tr>
            <td class="center vert-center">
              <a rel="item=<%=  item[1]["id"] %>&amp;ench=<%= item[1]["tooltipParams"]["enchant"] %>&amp;gems=<%= item[1]["tooltipParams"]["gem0"] %>:<%= item[1]["tooltipParams"]["gem1"] %>:<%= item[1]["tooltipParams"]["gem2"] %>&amp;pcs=<%= item[1]["tooltipParams"]["set"].try(:join, ":") %>">
                <img src="http://media.blizzard.com/wow/icons/36/<%= item[1]["icon"] %>.jpg" alt="<%= item[1]["icon"] %>" class="border<%= item[1]["quality"] %>">
              </a>
            </td>
            <td class="center vert-center"><%= item[1]["itemLevel"] %></td>
            <td class="center vert-center"><%= item[1]["name"] %></td>
          </tr>
          <% end %>
        </table>
      </div>
    </div>
  </div>
</div>
